(query g
  (must (= step 0)
    (new e
      (set e.Pos.x 10
        (set e.Pos.y 20
          t)))))

(query e
  (must (< e.Pos.y 400)
    (set e.Pos.y (+ e.Pos.y (* 10 delta))
      (set e.Pos.x (+ e.Pos.x (* 5 delta))
        (draw! e
          t)))))

(query a
  (query b
    (must (!= a b)
      (set a.Pos.x (+ a.Pos.x (* 0.01 (- b.Pos.x a.Pos.x)))
        t))))
